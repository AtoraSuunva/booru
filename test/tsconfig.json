{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2018"
    ],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "lf",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noErrorTruncation": true,
    "removeComments": true,
    "alwaysStrict": true,
    "strict": true,
    "emitBOM": true,
    "declaration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}