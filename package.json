    "tbib",
    "xbooru",
    "paheal",
    "derpibooru"
  ],
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.7",
    "undici": "^7.22.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/node": "^24.10.13",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist/"
  ],
  "directories": {
    "lib": "src/",
    "test": "test/"
