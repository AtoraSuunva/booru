{
  "name": "booru",
  "version": "2.0.0",
  "description": "Search (and do other things) on a bunch of different boorus!",
  "author": "AtlasTheBot",
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "runkitExampleFilename": "./dist/example.js",
  "homepage": "https://github.com/AtlasTheBot/booru#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtlasTheBot/booru.git"
  },
  "bugs": {
    "url": "https://github.com/AtlasTheBot/booru/issues"
  },
  "scripts": {
    "test": "jest --no-cache",
    "lint": "tslint --fix -p . -c ./tslint.json ./src/**/*.ts",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean && npm run lint && npm test",
    "build": "tsc",
    "postbuild": "uglifyjs-folder ./dist -yeo ./dist -x .js && copyfiles ./example.js ./dist"
  },
  "keywords": [
    "booru",
    "e621",
    "hypnohub",
    "danbooru",
    "konachan",
    "yandere",
    "gelbooru",
    "rule34",
    "safebooru",
    "tbib",
    "xbooru",
    "youhateus",
    "dollbooru",
    "paheal",
    "lolibooru",
    "derpibooru"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "fast-xml-parser": "^3.12.13",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.4",
    "@types/node-fetch": "^2.1.6",
    "copyfiles": "^2.1.0",
    "jest": "^24.1.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.13.0",
    "typescript": "^3.3.3333",
    "uglifyjs-folder": "^1.5.1"
  },
  "files": [
    "dist/"
  ],
  "directories": {
    "doc": "./docs",
    "lib": "./src",
    "test": "./test"
  }
}
